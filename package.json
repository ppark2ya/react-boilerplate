{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react project used on webpack, swc, tailwind",
  "author": "jkpark.dev@gmail.com",
  "license": "MIT",
  "keywords": [
    "react18",
    "webpack",
    "swc",
    "tailwind",
    "serverless",
    "cloudformation"
  ],
  "scripts": {
    "start": "webpack serve",
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"./**/*.{ts,tsx,js,jsx}\" --ignore-path .eslintignore || true",
    "lint:fix": "eslint --fix \"./**/*.{ts,tsx,js,jsx}\" --ignore-path .eslintignore || true",
    "lint:style": "stylelint ./**/*.{css,scss,pcss} --fix",
    "lint:commit": "commitlint --edit",
    "prettier": "prettier --write --config ./.prettierrc \"./src/**/*.{ts,tsx,js,jsx}\" || true",
    "deploy:dev": "sls deploy",
    "deploy:prod": "sls deploy --stage prod",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "src/**": [
      "npm run prettier"
    ],
    "./**/*.{ts,tsx,js,jsx}": [
      "npm run lint:fix"
    ],
    "./**/*.{css,scss,pcss}": [
      "npm run lint:style"
    ]
  },
  "dependencies": {
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "react": "18.0.0-rc.2",
    "react-dom": "18.0.0-rc.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@svgr/webpack": "^6.2.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.130",
    "@swc/helpers": "^0.3.8",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "css-loader": "^6.5.1",
    "cssnano": "^5.1.3",
    "dart-sass": "^1.25.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fibers": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.5",
    "mini-css-extract-plugin": "^2.5.2",
    "msw": "^0.39.2",
    "open-color": "^1.9.1",
    "pinst": "^3.0.0",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-focus-visible": "^6.0.4",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.2.3",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "serverless": "^3.7.4",
    "serverless-s3-sync": "^3.0.0",
    "serverless-stack-termination-protection": "^1.0.4",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-prettier": "^2.0.0",
    "swc-loader": "^0.1.15",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
